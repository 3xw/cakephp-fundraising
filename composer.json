{
  "name": "3xw/cakephp-fundraising",
  "description": "CakePHP fundraising plugin",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Fundraising\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Fundraising\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
